{
    "name": "duckduckgo-privacy-extension",
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    },
    "license": "Apache-2.0",
    "repository": "duckduckgo/duckduckgo-privacy-extension",
    "scripts": {
        "postinstall": "git submodule update --init --recursive && npm run bundle-config && npm run copy-sjcl && export PATH='$PATH:$PWD/node_modules/chromedriver/bin'",
        "bundle-config": "node scripts/bundleConfig.mjs",
        "copy-sjcl": "cd node_modules/sjcl/ && ./configure --compress=none --without-all --with-hmac --with-codecHex && make && cp sjcl.js ../../shared/js/content-scope/",
        "build": "echo 'Try npm run release-firefox or npm run release-chrome instead' && exit 0",
        "dev": "echo 'Try npm run dev-firefox or npm run dev-chrome instead' && exit 0",
        "release": "grunt build && node scripts/httpsAllowlist.js",
        "selenium": "cd selenium-test && node test.js",
        "lint": "eslint shared/js unit-test integration-test scripts",
        "lint-fix": "eslint shared/js unit-test integration-test --fix",
        "test": "grunt test --browser=chrome --type=dev",
        "test-debug": "grunt test --browser=chrome --type=dev --test-debug",
        "test-int": "make dev browser=chrome type=dev && jasmine --config=integration-test/config.json",
        "test-int-x": "xvfb-run --server-args='-screen 0 1024x768x24' npm run test-int",
        "test-ci": "npm run lint && npm test && npm run test-int-x",
        "dev-firefox": "make dev browser=firefox type=dev watch=1",
        "open-dev-firefox": "web-ext run -s build/firefox/dev/ -u https://privacy-test-pages.glitch.me/",
        "release-firefox": "make browser=firefox type=release",
        "dev-chrome": "make dev browser=chrome type=dev watch=1",
        "beta-firefox": "make beta-firefox browser=firefox type=release",
        "release-chrome": "make browser=chrome type=release && make chrome-release-zip"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/node": "^7.15.4",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.15.6",
        "@fingerprintjs/fingerprintjs": "^3.3.0",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "@rollup/plugin-replace": "^3.0.0",
        "babelify": "10.0.0",
        "duckduckgo-colors": "0.0.1",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "grunt": "1.4.1",
        "grunt-browserify": "6.0.0",
        "grunt-contrib-concat": "1.0.1",
        "grunt-contrib-watch": "1.1.0",
        "grunt-exec": "3.0.0",
        "grunt-execute": "0.2.2",
        "grunt-karma": "4.0.2",
        "grunt-sass": "3.1.0",
        "jasmine": "3.9.0",
        "jasmine-core": "3.5.0",
        "karma": "6.3.4",
        "karma-chrome-launcher": "3.1.0",
        "karma-jasmine": "1.1.1",
        "karma-source-map-support": "1.4.0",
        "load-grunt-tasks": "5.1.0",
        "node-fetch": "^3.0.0",
        "puppeteer": "10.4.0",
        "request": "^2.88.2",
        "rollup": "^2.57.0",
        "rollup-plugin-dynamic-import-variables": "^1.1.0",
        "sass": "^1.42.1",
        "standard": "16.0.3",
        "timekeeper": "^2.1.1",
        "webextension-polyfill": "^0.8.0",
        "yargs": "^17.2.1"
    },
    "dependencies": {
        "@duckduckgo/autofill": "https://github.com/duckduckgo/duckduckgo-autofill.git#3.2.4",
        "@duckduckgo/jsbloom": "^1.0.2",
        "@duckduckgo/privacy-grade": "1.0.3",
        "bel": "6.0.0",
        "deep-freeze": "0.0.1",
        "dexie": "3.0.3",
        "eventemitter2": "6.4.4",
        "is-plain-object": "5.0.0",
        "jquery": "3.6.0",
        "node-gyp": "^8.2.0",
        "normalize.scss": "0.1.0",
        "object.values": "1.1.4",
        "parse-link-header": "1.0.1",
        "psl": "1.8.0",
        "punycode": "2.1.1",
        "runtimer": "1.0.3",
        "seedrandom": "^3.0.5",
        "sjcl": "^1.0.8",
        "tldts": "^5.7.47",
        "underscore": "1.13.1",
        "web-ext": "^6.4.0"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "exclude": ["transform-regenerator"],
                    "debug": true
                }
            ]
        ]
    }
}
